      <data key="d12">1ef48284d238405f94190125092a3e28</data>
      <data key="d13">216</data>
      <data key="d14">1</data>
    </edge>
    <edge source="GENERATEREPORTS" target="EMBEDGRAPH">
      <data key="d9">1.0</data>
      <data key="d10">GenerateReports may be followed by EmbedGraph in a workflow, where reports are used to inform the embedding of graph structures into a vector space</data>
