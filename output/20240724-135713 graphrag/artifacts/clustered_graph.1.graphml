      <data key="d12">3f8b5b2727924ba0b62e6286063b6861</data>
      <data key="d13">200</data>
      <data key="d14">1</data>
    </edge>
    <edge source="GRAPHRAG INDEXING PIPELINE" target="GENERATEREPORTS">
