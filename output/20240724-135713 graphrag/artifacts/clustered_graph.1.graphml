      <data key="d12">64961fbc3a1641378be10bcb3b0955e1</data>
      <data key="d13">209</data>
      <data key="d14">1</data>
    </edge>
    <edge source="GRAPHRAG'S INDEXING PIPELINE" target="WORKFLOW GRAPHS">
      <data key="d9">1.0</data>
      <data key="d10">GraphRAG's Indexing Pipeline uses Workflow Graphs to represent the complex interdependencies between multiple workflows, forming a DAG for scheduling processing</data>
