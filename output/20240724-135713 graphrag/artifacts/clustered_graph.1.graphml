      <data key="d12">8806b817446447e3b50f5bc85ff497e1</data>
      <data key="d13">217</data>
      <data key="d14">1</data>
    </edge>
    <edge source="EMBEDGRAPH" target="ENTITYRESOLUTION">
      <data key="d9">1.0</data>
      <data key="d10">EmbedGraph may be followed by EntityResolution in a workflow, where embedded graph structures are used to resolve entities within the data</data>
