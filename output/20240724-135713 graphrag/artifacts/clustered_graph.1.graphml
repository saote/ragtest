      <data key="d12">029d1a8c3b184aa5bb21228f40cd12fd</data>
      <data key="d13">194</data>
      <data key="d14">1</data>
    </edge>
    <edge source="WORKFLOW" target="VERB">
      <data key="d9">1.0</data>
      <data key="d10">A Workflow in DataShaper is composed of Verbs, which are steps that model relational concepts and transform input data tables</data>
