      <data key="d12">f3018b934ac241639a33c925c24bc507</data>
      <data key="d13">212</data>
      <data key="d14">1</data>
    </edge>
    <edge source="CHUNK" target="EXTRACTGRAPH">
      <data key="d9">1.0</data>
      <data key="d10">Chunk may be followed by ExtractGraph in a workflow, where data chunks are analyzed to extract graph structures</data>
