      <data key="d12">6ed8b67be79242e98aa1b9283431d5df</data>
      <data key="d13">641</data>
      <data key="d14">1</data>
    </edge>
    <edge source="CLI ARGUMENTS" target="EMIT">
      <data key="d9">1.0</data>
      <data key="d10">The emit argument is a CLI Argument that determines the output formats for the tables generated by the GraphRAG Indexer CLI.</data>
