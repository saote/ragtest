      <data key="d12">acb53370e72b4430a752d9ea18c17352</data>
      <data key="d13">198</data>
      <data key="d14">1</data>
    </edge>
    <edge source="GRAPHRAG INDEXING PIPELINE" target="EXTRACTGRAPH">
      <data key="d9">1.0</data>
      <data key="d10">The ExtractGraph step is part of the GraphRAG Indexing Pipeline, where it is used to extract graph structures from data.</data>
