      <data key="d12">f745075dedcf444daa9370cf32403d31</data>
      <data key="d13">215</data>
      <data key="d14">1</data>
    </edge>
    <edge source="EMBEDDOCUMENTS" target="GENERATEREPORTS">
      <data key="d9">1.0</data>
      <data key="d10">EmbedDocuments may be followed by GenerateReports in a workflow, where embedded documents are used to generate reports based on the processed data</data>
