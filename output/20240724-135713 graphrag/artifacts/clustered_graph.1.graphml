      <data key="d12">ee91a06f13b4495f95c800a0c7329ef7</data>
      <data key="d13">640</data>
      <data key="d14">1</data>
    </edge>
    <edge source="CLI ARGUMENTS" target="REPORTER">
      <data key="d9">1.0</data>
      <data key="d10">The reporter argument is a CLI Argument that specifies the progress reporting mechanism for the GraphRAG Indexer CLI.</data>
