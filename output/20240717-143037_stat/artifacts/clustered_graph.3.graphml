    <edge source="HAT_MATRIX" target="PROPERTIES_HAT_MATRIX">
      <data key="d9">1.0</data>
      <data key="d10">The hat matrix has specific properties that are important for understanding its role in regression analysis</data>
      <data key="d11">1d52aaeb960f9787b6229e57738f8e47</data>
      <data key="d12">19aa5f0b738c4f4a96668c80c3e93331</data>
